{
    "name": "kntista-web",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "bun run eslint --fix .",
        "format": "bun run --bun prettier . --write --ignore-path ../.gitignore",
        "dev": "bun run --bun --env-file ../.env react-router dev",
        "build": "bun run --bun --env-file ../.env react-router build",
        "start": "bun run --bun react-router-serve ./build/server/index.js",
        "typecheck": "bun run --bun react-router typegen && bun run --bun tsc"
    },
    "dependencies": {
        "@react-router/node": "7.10.1",
        "@react-router/serve": "7.10.1",
        "isbot": "^5.1.31",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "react-router": "7.10.1"
    },
    "devDependencies": {
        "@eslint/compat": "^2.0.0",
        "@react-router/dev": "7.10.1",
        "@tailwindcss/vite": "^4.1.13",
        "@types/node": "^22",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@typescript-eslint/parser": "^8.51.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-better-tailwindcss": "^3.8.0",
        "prettier": "^3.7.4",
        "prettier-plugin-organize-imports": "^4.3.0",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "tailwindcss": "^4.1.13",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.51.0",
        "vite": "^7.1.7",
        "vite-tsconfig-paths": "^5.1.4"
    }
}
