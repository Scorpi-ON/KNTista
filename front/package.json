{
    "name": "kntista-web",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "bun run -b eslint --fix .",
        "format": "bun run -b prettier . --write --ignore-path ../.gitignore",
        "dev": "bun run -b --env-file ../.env react-router dev",
        "build": "bun run -b --env-file ../.env react-router build",
        "start": "bun run -b react-router-serve ./build/server/index.js",
        "typecheck": "bun run -b react-router typegen && bun run -b tsc"
    },
    "dependencies": {
        "@heroui/react": "^3.0.0-beta.5",
        "@heroui/styles": "^3.0.0-beta.5",
        "@react-router/node": "7.13.0",
        "@react-router/serve": "7.13.0",
        "better-auth": "1.4.18",
        "isbot": "^5.1.31",
        "joi": "^18.0.2",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-router": "7.13.0"
    },
    "devDependencies": {
        "@eslint/compat": "^2.0.0",
        "@react-router/dev": "7.13.0",
        "@tailwindcss/vite": "^4.1.18",
        "@types/node": "^22",
        "@types/react": "^19.2.10",
        "@types/react-dom": "^19.2.3",
        "@typescript-eslint/parser": "^8.54.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-better-tailwindcss": "^4.0.2",
        "prettier": "^3.8.1",
        "prettier-plugin-organize-imports": "^4.3.0",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.54.0",
        "vite": "^7.3.1",
        "vite-tsconfig-paths": "^6.0.5"
    }
}
